package tstiop_void_type;

union VoidAlone {
    void field;
    int other;
};

/* try conversion : anything to void */
struct IntToVoid {
    int field;
};
struct ArrayToVoid {
    int[] field;
};
struct SmallArrayToVoid {
    byte[] field;
};
struct StructToVoid {
    IntToVoid field;
};
struct DoubleToVoid {
    double field;
};
struct VoidRequired {
    void field;
};
struct VoidOptional {
    void? field;
};
