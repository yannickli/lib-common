package snmp_test_doc;

enum TestDocEnum {
    STATE_ONE,
    STATE_TWO,
    STATE_THREE,
    FOUR,
};

@snmpParam
snmpObj Obj : 1 : snmp_intersec_test.Test {
    /** Description, ex: "some string".
     */
1:  string a;

    /** Description.
     */
2:  TestDocEnum b;
};

/** Table.
 */
snmpTbl Tbl : 3 : Obj {
     /** Description.
      */
1:   string column1;

     /** Description.
      */
2:   int column2;

     /** Description.
      */
     @snmpIndex
3:   uint indexTbl;
};

@snmpParamsFrom(Obj)
snmpIface Iface : 4 : Obj {
    /** Description ex: "some notif".
     */
1:  notifUnresponsive
        in (a, b);

    /** Description.
     */
2:  notifResponsive
        in (a);
};

/** Table with indexes.
 */
snmpTbl TblWithIndex : 5 : Obj {
     /** Description.
      */
     @snmpIndex
1:   string index1;

     /** Description.
      */
     @snmpIndex
2:   uint index2;

     /** Description.
      */
3:   int notIndex;
};
